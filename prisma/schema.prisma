// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model SmscStat {
  id   Int      @id @default(autoincrement())
  date DateTime @unique
  suspiciousCount Int
  totalCount Int
}
